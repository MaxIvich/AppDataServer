<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.appdataserver.AuthorisationController"
            prefHeight="400.0" prefWidth="600.0">
    <HBox fx:id="authBox" >
        <TextField fx:id="loginField" promptText="login"/>
        <PasswordField fx:id="passField" promptText="password"/>
        <Button text="Sign in" onAction="#signInBtnClick"/>
        <Button text="Reg" onAction="#RegBtnClick"/>
    </HBox>

</AnchorPane>
